import java.util.*;
import java.util.Scanner;
class sanji
{
    public static void main(String args[])
    {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int sum=0;
        for(int i=1;i<=n;i++)
        {
            if(n%i==0)
            sum=sum+i;
        }
        if(sum==(2*n))
        {
            System.out.println("YES PERFECT");
        }
        else{
            System.out.println("NO NOT PERFECT");
        }
    }
}
