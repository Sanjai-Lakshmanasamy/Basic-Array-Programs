import java.util.*;
import java.util.Scanner;
class sanji
{
    public static void main(String args[])
    {
        Scanner s=new Scanner(System.in);
        int n=s.nextInt();
        int a[]=new int[n];
        for(int i=0;i<n;i++)
        {
            a[i]=s.nextInt();
        }
        for(int i=1;i<a.length;i=i+2) //PUT I=0 FOR ODD POSITION ELEMENTS
        {
            System.out.println(a[i]); //CAN USE if(a[i]%2==0) LOGIC ALSO
        }
         
    }
}
